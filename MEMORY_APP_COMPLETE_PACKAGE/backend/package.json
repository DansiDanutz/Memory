{
  "name": "memo-app-backend",
  "version": "1.0.0",
  "description": "Memo - Personal AI Brain Backend with HMAC-SHA256 Security",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest tests/security",
    "test:performance": "jest tests/performance",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "npx prisma db seed",
    "db:vacuum": "node scripts/db-maintenance.js",
    "db:analyze": "node scripts/db-analyze.js",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "node scripts/security-scan.js",
    "deploy:staging": "bash scripts/deploy-staging.sh",
    "deploy:production": "bash scripts/deploy-production.sh",
    "logs:tail": "tail -f logs/memo-app.log",
    "metrics:reset": "curl -X POST http://localhost:3001/admin/metrics/reset",
    "health:check": "curl -f http://localhost:3001/health"
  },
  "keywords": [
    "memo",
    "ai",
    "memory",
    "whatsapp",
    "hmac",
    "security",
    "api",
    "backend"
  ],
  "author": "Memo App Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.2.0",
    "node-crypto": "^1.0.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "axios": "^1.6.2",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "@prisma/client": "^5.7.0",
    "prisma": "^5.7.0",
    "pg": "^8.11.3",
    "openai": "^4.20.1",
    "pinecone-client": "^1.1.0",
    "whatsapp-web.js": "^1.23.0",
    "qrcode": "^1.5.3",
    "puppeteer": "^21.6.1",
    "ffmpeg-static": "^5.2.0",
    "speech-to-text": "^2.0.0",
    "text-to-speech": "^1.0.0",
    "natural": "^6.8.0",
    "compromise": "^14.10.0",
    "sentiment": "^5.0.2",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "validator": "^13.11.0",
    "sanitize-html": "^2.11.0",
    "dompurify": "^3.0.6",
    "jsdom": "^23.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.11",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/validator": "^13.11.7",
    "@types/sanitize-html": "^2.9.5",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/memo-app/backend.git"
  },
  "bugs": {
    "url": "https://github.com/memo-app/backend/issues"
  },
  "homepage": "https://memo-app.com"
}

